<%= form_for(@entry) do |f| %>
  
  <div class="field">
    <%= f.label :food %>
    <%= f.text_field :food %>
  </div>
  
  <div class="field">
    <%= f.label :amount %>
    <%= f.text_field :amount %>
  </div>
  
  <div class="field">
    <%= f.label :meal_time %>
    <%= f.text_field :meal_time %>
  </div>
  
  <div class="field">
    <%= f.label :location %>
    <%= f.text_field :location %>
  </div>
  
  <div class="field">
    <%= f.label :sensory %>
    <%= f.text_field :sensory %>
  </div>
  
  <div class="field">
    <%= f.label :emotional %>
    <%= f.text_field :emotional %>
  </div>
  
  <%= f.submit %><%= link_to 'Cancel' %>
<% end %>